layer {
  name: "score_sum"
  type: "Eltwise"
  bottom: "score_fr37_data0"
  bottom: "score_fr37_data1"
  propagate_down: false
  propagate_down: false
  top: "score_sum"
  eltwise_param {
    operation: SUM
    coeff: 0.5
    coeff: 0.5
  }
}
layer {
  name: "upscore_sum"
  type: "Deconvolution"
  bottom: "score_sum"
  top: "upscore_sum"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 37
    bias_term: false
    kernel_size: 64
    stride: 32
  }
}

layer {
  name: "final_score_sum"
  type: "Crop"
  bottom: "upscore_sum"
  bottom: "data1"
  top: "final_score_sum"
  crop_param {
    axis: 2
    offset: 19
  }
}

#########################################
#########################################
################ FUSION BY CONCAT #######
#########################################
#########################################
layer {
  name: "score_concat"
  type: "Concat"
  bottom: "score_fr37_data0"
  bottom: "score_fr37_data1"
  propagate_down: false
  propagate_down: false
  top: "score_concat"
  concat_param {
    axis: 1
  }
}
layer {
  name: "score_fused_concat"
  type: "Convolution"
  bottom: "score_concat"
  top: "score_fused_concat"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 37
    pad: 0
    kernel_size: 1
    weight_filler {
      type: "constant"
      value: 0.0135
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "upscore_concat"
  type: "Deconvolution"
  bottom: "score_fused_concat"
  top: "upscore_concat"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 37
    bias_term: false
    kernel_size: 64
    stride: 32
  }
}
layer {
  name: "final_score_concat"
  type: "Crop"
  bottom: "upscore_concat"
  bottom: "data1"
  top: "final_score_concat"
  crop_param {
    axis: 2
    offset: 19
  }
}

#########################################
#########################################
##### RC CONCAT #####
#########################################
#########################################
layer {
  name: "rc_score_concat"
  type: "Concat"
  bottom: "score_fr37_data0"
  bottom: "score_fr37_data1"
  propagate_down: false
  propagate_down: false
  top: "rc_score_concat"
  concat_param {
    axis: 1
  }
}
layer {
  name: "score_concat_rc_conv1"
  type: "Convolution"
  bottom: "rc_score_concat"
  top: "score_concat_rc_conv1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 37
    pad: 0
    kernel_size: 1
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.0001
    }
  }
}
layer {
  name: "score_concat_rc_conv3"
  type: "Convolution"
  bottom: "rc_score_concat"
  top: "score_concat_rc_conv3"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 37
    pad: 1
    stride:1
    kernel_size: 3
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.0001
    }
  }
}
layer {
  name: "score_concat_rc_conv5"
  type: "Convolution"
  bottom: "rc_score_concat"
  top: "score_concat_rc_conv5"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 37
    pad: 2
    stride:1
    kernel_size: 5
    weight_filler {
      type: "msra"
    }
    bias_filler {
      type: "constant"
      value: 0.0001
    }
  }
}
layer {
  name: "rc_score_correction"
  bottom: "score_concat_rc_conv1"
  bottom: "score_concat_rc_conv3"
  bottom: "score_concat_rc_conv5"
  top: "rc_score_correction"
  type: "Eltwise"
  eltwise_param {
    operation: SUM
    coeff: 0.33333
    coeff: 0.33333
    coeff: 0.33333
  }
}
#########################################
#########################################
################ RC SUM ##########
#########################################
#########################################

layer {
  name: "rc_score_average"
  type: "Eltwise"
  bottom: "score_fr37_data0"
  bottom: "score_fr37_data1"
  propagate_down: false
  propagate_down: false
  top: "rc_score_average"
  eltwise_param {
    operation: SUM
    coeff: 0.5
    coeff: 0.5
  }
}
layer {
  name: "rc_score_corrected"
  type: "Eltwise"
  bottom: "rc_score_average"
  bottom: "rc_score_correction"
  top: "rc_score_corrected"
  eltwise_param {
    operation: SUM
  }
}
layer {
  name: "upscore_rc_corrected"
  type: "Deconvolution"
  bottom: "rc_score_corrected"
  top: "upscore_rc_corrected"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 37
    bias_term: false
    kernel_size: 64
    stride: 32
  }
}

layer {
  name: "final_score_rc"
  type: "Crop"
  bottom: "upscore_rc_corrected"
  bottom: "data0"
  top: "final_score_rc"
  crop_param {
    axis: 2
    offset: 19
  }
}
